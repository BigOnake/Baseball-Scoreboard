                            <Line X1="0" Y1="125" X2="300" Y2="125" Stroke="Black" StrokeThickness="1" Canvas.Left="-243" Canvas.Top="24"/>
                            <Line X1="0" Y1="150" X2="300" Y2="150" Stroke="Black" StrokeThickness="1" Canvas.Left="259"/>
                            <Line X1="0" Y1="175" X2="300" Y2="175" Stroke="Black" StrokeThickness="1" Canvas.Left="-307" Canvas.Top="25"/>
