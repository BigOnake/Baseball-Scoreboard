                            <Line X1="0" Y1="200" X2="300" Y2="200" Stroke="Black" StrokeThickness="1" Canvas.Left="-226" Canvas.Top="45"/>
                            <Line X1="0" Y1="200" X2="300" Y2="200" Stroke="Black" StrokeThickness="1" Canvas.Left="236" Canvas.Top="50"/>
                        </Canvas>
                    </Viewbox>

                    <Viewbox Stretch="Uniform">
                        <Rectangle Width="100" Height="100"
                            Fill="White"
                            Stroke="Black"
                            StrokeThickness="1">
                            <Rectangle.RenderTransform>
                                <TransformGroup>
